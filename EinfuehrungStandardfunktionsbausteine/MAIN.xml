        <body>
          <ST>
            <xhtml xmlns="http://www.w3.org/1999/xhtml">// Hier den Code ändern

// TASTER
	// Counter erhöhen, wenn Taster S10 gedrückt
	IF bS10 THEN
		nCount := LIMIT(0,nCount + 1,10);
	END_IF
	// Counter verringern, wenn Taster S11 gedrückt
	IF bS11 THEN
		nCount := LIMIT(0,nCount - 1,10);
	END_IF

// LEDs
	bL10 := bS10;
	bL11 := bS10;
	bL12 := bS10;
	bL13 := bS10;
	
	
	

</xhtml>
          </ST>
        </body>
